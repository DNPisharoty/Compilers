VAR myvar;
VAR othervar;
VAR anothervar;
VAR pi;
VAR index;

pi = 3.14;


GO 27-7+myvar;

TURN othervar;

FOR index = -1 
    STEP +1 
    TO 90-1 
    DO 
       GO index;

BEGIN
myvar = 22-pi;
JUMP myvar;
END

PROCEDURE a
{
IF (myvar > pi) THEN 
  {
  
  }
ELSE

{
   GO dist;
}
index = 0;
WHILE(index<10)
{
JUMP 270;
TURN index + 3;
i=i+1;
}
}

PROCEDURE bettera
{
  FOR index = 3
     STEP 5
     TO 90
     DO
       TURN index;
       GO index;
}

CALL a;
 
