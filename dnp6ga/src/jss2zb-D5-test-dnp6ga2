VAR myvar;
VAR othervar;
VAR anothervar;
VAR pi;
VAR index;

pi = 3.14;


GO 27-7+myvar;
TURN othervar;

FOR index = -1 
    STEP +1 
    TO 90-1 
    DO 
       GO index;

BEGIN
myvar = 22-pi;
JUMP myvar;

PROCEDURE bettera
{
  FOR index = 3
     STEP 5
     TO 90
     DO
       TURN index;
       GO index;
}
