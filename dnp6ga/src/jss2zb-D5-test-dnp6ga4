VAR myvar;
VAR othervar;
VAR anothervar;
VAR pi;
VAR index;

pi = 3.14;


GO 27-7+myvar;
TURN othervar;

FOR index = -1 
    STEP +1 
    TO 90-1 
    DO 
       GO index;

BEGIN
myvar = 22-pi;
JUMP myvar;
END

IF (myvar > pi) THEN 
  {
   IF(othervar == 12.45) 
   {
     GO myvar;
	 }
   ELSE
    { 
     GO dist;
    }
  }
ELSE
{
   GO dist;
}
    


 
